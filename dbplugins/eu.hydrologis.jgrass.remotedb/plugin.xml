<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="eu.hydrologis.jgrass.remotedb.preferences.DbPreferencePage"
            id="embedded.ui.preferences.RemoteDbPreferencePage"
            name="Remote Database Server">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="eu.hydrologis.jgrass.remotedb.preferences.DbPreferencesInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="eu.hydrologis.jgrass.remotedb.view.DatabaseView"
            icon="icons/db.gif"
            id="eu.hydrologis.jgrass.embeddeddb.dbviewremote"
            name="Remote Database View">
      </view>
   </extension>
</plugin>
