<form>
    <tab order="0" name="tab1" text="Informazioni Geosito" layout="insets 20 20 20 20, fillx" col="[left][fill][left][fill]">
        <label order="0" name="label0" text="0 - Identificativo scheda" constraints=""/>
        <label order="1" name="label1" text="Compilatore/Ente" constraints="growx"/>
        <label order="2" name="label2" text="Acqusizione Dati" constraints="growx"/>
        <label order="3" name="label3" text="Codice Scheda" constraints="growx"/>
        <label order="4" name="label4" text="Data Scheda" constraints="growx"/>
        <label order="5" name="label5" text="Scheda Collegata" constraints="growx, wrap"/>
        
        <textfield order="6" name="text1" default="ARPA" fieldname="ente" valuetype="string" constraints="skip 1, growx"/>
        <combobox order="7" name="combo1" default="2" fieldname="acqdati" constraints="growx">
            <item>Rilev., 1</item>
            <item>Bibliogr., 2</item>
        </combobox>
        <textfield order="8" name="text2" default="E101" fieldname="codscheda" valuetype="string" constraints="growx"/>
        <textfield order="9" name="text3" default="2010-09-07" fieldname="data" valuetype="string" constraints="growx"/>
        <radiobutton order="10" name="radio1" default="1" fieldname="collegata" constraints="growx, wrap">
            <item>si, 1</item>
            <item>no, 2</item>
        </radiobutton>
        
        <separator order="11" constraints="span,growx, wrap"/>
        <separator order="11.1" orientation="horizontal" constraints="span,growx, wrap"/>
        <separator order="11.2" constraints="span,growx, wrap"/>
        
        <label order="12" name="label6" text="A - Nome del geosito" constraints=""/>
        <textfield order="13" name="text4" default="il loco" fieldname="nomegeos" valuetype="string" constraints="span 5, growx, wrap"/>
        
        <separator order="13.1" constraints="span,growx, wrap"/>
        <separator order="13.2" orientation="horizontal" constraints="span,growx, wrap"/>
        <separator order="13.3" constraints="span,growx, wrap"/>

        <label order="14" name="label7" text="B - Ubicazione" constraints="span 4, growx"/>
        <label order="15" name="label8" text="Coordinate Geografiche - UTM, Gauss-Boaga" constraints="span 3, growx, wrap"/>
        
        <label order="16" name="label9" text="Regione" constraints="skip 1, growx"/>
        <textfield order="17" name="text5" default="TAA" fieldname="regione" valuetype="string" constraints="growx"/>
        <label order="17.1" name="label9.1" text="" constraints="growx"/>
        <label order="18" name="label10" text="Longitudine" constraints="growx"/>
        <textfield order="19" name="text6" default="" fieldname="lon" valuetype="double" constraints="growx, wrap"/>
        
        <label order="20" name="label11" text="Provincia" constraints="skip 1, growx"/>
        <textfield order="21" name="text7" default="BZ" fieldname="provincia" valuetype="string" constraints="growx"/>
        <label order="21.1" name="label11.1" text="" constraints="growx"/>
        <label order="22" name="label12" text="Latitudine" constraints="growx"/>
        <textfield order="23" name="text8" default="" fieldname="lat" valuetype="double" constraints="growx, wrap"/>
        
        <label order="24" name="label13" text="Comune" constraints="skip 1, growx"/>
        <textfield order="25" name="text9" default="Lagundo" fieldname="comune" valuetype="string" constraints="growx, wrap"/>
        
        <label order="26" name="label14" text="Toponimo/Localita" constraints="skip 1, growx"/>
        <textfield order="27" name="text10" default="Lagundo" fieldname="localita" valuetype="string" constraints="growx"/>
        <label order="27.1" name="label14.1" text="" constraints="growx"/>
        <label order="28" name="label15" text="Quota Max" constraints="growx"/>
        <textfield order="29" name="text11" default="" fieldname="quotamax" valuetype="double" constraints="growx, wrap"/>

        <label order="30" name="label16" text="Quota Min" constraints="skip 4, growx"/>
        <textfield order="31" name="text12" default="" fieldname="quotamin" valuetype="double" constraints="growx, wrap"/>

        <label order="32" name="label17" text="Quota Media" constraints="skip 4, growx"/>
        <textfield order="33" name="text13" default="" fieldname="quotamed" valuetype="double" constraints="growx, wrap"/>
        
    </tab>
    <tab order="1" name="tab2" text="Interesse scientifico" layout="insets 20 20 20 20, fillx" col="[right][fill][right][fill][right][fill]">
        <label order="0" name="label0" text="C - Interesse Scientifico" constraints="growx, wrap"/>
        
        <label order="1" name="label1" text="Geografico" constraints=""/>
        <combobox order="2" name="combo1" default="" fieldname="geogr" constraints="">
            <item>Primario, 1</item>
            <item>Secondario, 2</item>
        </combobox>
        <label order="3" name="label2" text="Idrogeologico" constraints=""/>
        <combobox order="4" name="combo2" default="" fieldname="idrogeol" constraints="">
            <item>Primario, 1</item>
            <item>Secondario, 2</item>
        </combobox>
        <label order="5" name="label3" text="Geologia marina" constraints=""/>
        <combobox order="6" name="combo3" default="" fieldname="geolmar" constraints="wrap">
            <item>Primario, 1</item>
            <item>Secondario, 2</item>
        </combobox>
        
        <label order="7" name="label4" text="Mineralogico" constraints=""/>
        <combobox order="8" name="combo4" default="" fieldname="mineral" constraints="">
            <item>Primario, 1</item>
            <item>Secondario, 2</item>
        </combobox>
        <label order="9" name="label5" text="Geologia Stratigrafica" constraints=""/>
        <combobox order="10" name="combo5" default="" fieldname="geolstrat" constraints="">
            <item>Primario, 1</item>
            <item>Secondario, 2</item>
        </combobox>
        <label order="11" name="label6" text="Naturalistico" constraints=""/>
        <combobox order="12" name="combo6" default="" fieldname="natural" constraints="wrap">
            <item>Primario, 1</item>
            <item>Secondario, 2</item>
        </combobox>
        <label order="13" name="label7" text="Geominerario" constraints=""/>
        <combobox order="14" name="combo7" default="" fieldname="geomin" constraints="wrap">
            <item>Primario, 1</item>
            <item>Secondario, 2</item>
        </combobox>

        <separator order="14.1" constraints="span,growx, wrap"/>
        <separator order="14.2" constraints="span,growx, wrap"/>

        <label order="15" name="label8" text="C1 - Altro Tipo di Interesse" constraints="growx, wrap"/>
        <label order="16" name="label9" text="Culturale" constraints=""/>
        <combobox order="17" name="combo8" default="" fieldname="cultural" constraints="wrap">
            <item>Primario, 1</item>
            <item>Secondario, 2</item>
        </combobox>
        <label order="18" name="label10" text="Didattico" constraints=""/>
        <combobox order="19" name="combo9" default="" fieldname="didatt" constraints="wrap">
            <item>Primario, 1</item>
            <item>Secondario, 2</item>
        </combobox>
        <label order="20" name="label11" text="Escursionistico" constraints=""/>
        <combobox order="21" name="combo10" default="" fieldname="escurs" constraints="wrap">
            <item>Primario, 1</item>
            <item>Secondario, 2</item>
        </combobox>

        <separator order="21.1" constraints="span,growx, wrap"/>
        <separator order="21.2" constraints="span,growx, wrap"/>
        
        <label order="22" name="label12" text="C2 - Valutazione Interesse Scientifico Primario" constraints="growx, span 3"/>
        <label order="23" name="label13" text="C3 - Grado Interesse Scientifico Primario" constraints="growx, span 3, wrap"/>
        <combobox order="24" name="combo11" default="" fieldname="intscienp" constraints="growx">
            <item>Raro, 1</item>
            <item>Rappresentativo, 2</item>
            <item>Esemplif., 3</item>
        </combobox>
        <combobox order="25" name="combo12" default="" fieldname="gintscienp" constraints="skip 2, growx, wrap">
            <item>Mondiale, 1</item>
            <item>Europeo, 2</item>
            <item>Nazionale, 3</item>
            <item>Regionale, 4</item>
            <item>Locale, 5</item>
        </combobox>

        <separator order="25.1" constraints="span,growx, wrap"/>
        <separator order="25.2" constraints="span,growx, wrap"/>


        <label order="26" name="label14" text="C4 - Il giudizio espresso in C e':" constraints="growx, wrap"/>
        <label order="27" name="label15" text="Soggettivo" constraints=""/>
        <checkbox order="28" name="check1" default="false" fieldname="soggettivo" constraints="growx, wrap"/>
        <label order="29" name="label16" text="Oggettivo (spiegare)" constraints="aligny top"/>
        <textarea order="30" name="textarea1" default="" fieldname="oggettivo" constraints="height 150, spanx 5, growx, growy, wrap"/>
        
    </tab>
    <tab order="2" name="tab3" text="Descrizione" layout="insets 20 20 20 20, fillx" col="[fill]">
        <label order="0" name="label0" text="Descrizione dell'oggetto" constraints="growx, wrap"/>
        <textarea order="1" name="textarea0" default="" fieldname="descrogg" constraints="height 250, growy, growx, wrap"/>
    </tab>
</form>
